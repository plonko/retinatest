    <script src="../../media/js/jquery.reel.js"></script>
    
    <script type="text/javascript">
        $( '#detail_205' ).live( 'pageinit',function(event){


            var arr = [];
            for (var x=3090; x<= 3139; x++)
                arr.push("/media/images/50/" + "Image_" +x + ".jpg");
            
            $("#rotate").reel({ 
                frames: 50,
                suffix: "",
                images:arr,
                brake: 0.01, 
                velocity: 1, 
                preloader: 0
            });


        });
    </script>

